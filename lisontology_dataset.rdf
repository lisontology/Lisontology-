### EPISODE 1 RELATIONS ###

# Player
lis:Player1 rdf:type lis:Player ;
    lis:impersonates lis:MaxCaulfield .

# Chloe's Weed Event - Fixed missing period and added triggers for takeBlame
lis:chloesWeed lis:takesPlaceIn lis:ChloesRoom ;
    lis:involves lis:MaxCaulfield, lis:ChloePrice ;
    lis:isContainedIn lis:Episode1 ;
    lis:triggers lis:blameChloeWeed, lis:takeBlameChloeWeed ;
    lis:elicits lis:emotionalResponseAnxiety .

lis:emotionalResponseAnxiety rdf:type lis:EmotionalResponse ;
    lis:isDrivenBy lis:playerValueHonesty, lis:playerMotivationSelfPreservation, lis:playerEmotionAnxiety ;
    lis:influences lis:blameChloeWeed, lis:takeBlameChloeWeed .

# Fixed missing period and added player emotion types
lis:blameChloeWeed lis:leadsTo lis:chloeDisapproves ;
    lis:isChoiceOf lis:Player1 .

lis:chloeDisapproves lis:elicits lis:emotionalResponseGuilt ;
    lis:induces lis:ReflectionState1 .

lis:ReflectionState1 lis:isInfluencedBy lis:emotionalResponseGuilt ;
    lis:isDrivenBy lis:playerValueEmpathy, lis:playerEmotionGuilt, lis:playerMotivationRepairRelationship ;
    lis:resultsIn lis:ReconsiderChoice1 .

lis:ReconsiderChoice1 lis:rejectsChoice lis:blameChloeWeed ;
    lis:enables lis:RewindMechanic1 .

lis:RewindMechanic1 lis:resets lis:chloesWeed .

lis:takeBlameChloeWeed lis:leadsTo lis:chloeGladNoReport ;
    lis:isChoiceOf lis:Player1 ;
    lis:induces lis:ReflectionState2 .

lis:chloeGladNoReport lis:elicits lis:emotionalResponseRelief .

lis:emotionalResponseRelief lis:isDrivenBy lis:playerEmotionAffection, lis:playerValueMoralGrowth, lis:playerMotivationProtectingChloe .

lis:ReflectionState2 lis:resultsIn lis:AcceptChoice1 ;
    lis:isInfluencedBy lis:emotionalResponseRelief .

lis:AcceptChoice1 lis:marks lis:FinalDecision1 .

lis:FinalDecision1 lis:resolves lis:Episode1End .

lis:Episode1End rdf:type lis:GameEnd ;
    lis:triggers lis:internalizedMeaning1 .

# Daniel's Portrait Event - Fixed consistency
lis:danielDaCostaPortrait lis:takesPlaceIn lis:MainCampus ;
    lis:involves lis:MaxCaulfield, lis:DanielDaCosta ;
    lis:isContainedIn lis:Episode1 ;
    lis:triggers lis:acceptDanielPortrait ;
    lis:elicits lis:emotionalResponseFlattery .

lis:emotionalResponseFlattery rdf:type lis:EmotionalResponse ;
    lis:isDrivenBy lis:playerValueOpenness, lis:playerMotivationSupportiveness, lis:playerEmotionAppreciation ;
    lis:influences lis:acceptDanielPortrait .

lis:acceptDanielPortrait lis:leadsTo lis:danielPortraitPosted ;
    lis:isChoiceOf lis:Player1 .

lis:danielPortraitPosted lis:induces lis:ReflectionState3 ;
    lis:elicits lis:emotionalResponseValidation .

lis:emotionalResponseValidation rdf:type lis:EmotionalResponse ;
    lis:isDrivenBy lis:playerValueCreativity, lis:playerMotivationEncourageDanielWork, lis:playerEmotionPride .

lis:ReflectionState3 rdf:type lis:ReflectionState ;
    lis:isInfluencedBy lis:emotionalResponseValidation ;
    lis:resultsIn lis:AcceptChoice2 .

lis:AcceptChoice2 rdf:type lis:AcceptChoice ;
    lis:reaffirmsChoice lis:acceptDanielPortrait ;
    lis:marks lis:FinalDecision2 .

lis:FinalDecision2 rdf:type lis:FinalDecision ;
    lis:resolves lis:Episode1End .

# Additional Relations
lis:MaxCaulfield lis:hasValue lis:playerValueFear, lis:playerValueConfidence ;
    lis:makesChoice lis:blameChloeWeed, lis:acceptDanielPortrait .


