<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10/"
     xml:base="http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10/"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:prov="http://www.w3.org/ns/prov#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:creon="http://www.ontologydesignpatterns.org/ont/creativity/creon.owl#">
    <owl:Ontology rdf:about="http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10">
        <owl:imports rdf:resource="http://www.ontologydesignpatterns.org/ont/creativity/creon.owl"/>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.ontologydesignpatterns.org/ont/creativity/creon.owl#atLocation -->

    <owl:ObjectProperty rdf:about="http://www.ontologydesignpatterns.org/ont/creativity/creon.owl#atLocation"/>
    


    <!-- http://www.ontologydesignpatterns.org/ont/creativity/creon.owl#hasTrait -->

    <owl:ObjectProperty rdf:about="http://www.ontologydesignpatterns.org/ont/creativity/creon.owl#hasTrait"/>
    


    <!-- http://www.ontologydesignpatterns.org/ont/creativity/creon.owl#induces -->

    <owl:ObjectProperty rdf:about="http://www.ontologydesignpatterns.org/ont/creativity/creon.owl#induces"/>
    


    <!-- http://www.ontologydesignpatterns.org/ont/creativity/creon.owl#participantIn -->

    <owl:ObjectProperty rdf:about="http://www.ontologydesignpatterns.org/ont/creativity/creon.owl#participantIn"/>
    


    <!-- http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#culminates -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#culminates">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#EpisodeEnd"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#InternalizedMeaning"/>
        <rdfs:comment>Indicates that the episode ends with the formation of an internalized meaning for the player.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#elicits -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#elicits">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#Event"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#Outcome"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#RewindMechanic"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#EmotionalResponse"/>
        <rdfs:comment>Models how an event or outcome generates an emotional reaction in the player.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#enables -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#enables">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#ReconsiderChoice"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#RewindMechanic"/>
        <rdfs:comment>Represents the link between a reconsidered choice and the enabling of the rewind mechanic.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#enteredBy -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#enteredBy">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#ReflectionState"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#Player"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#has -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#has">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/creativity/creon.owl#hasTrait"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#Player"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#PlayerMotivation"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#PlayerValue"/>
        <rdfs:comment>Connects a player to their internal attributes—values, emotions, and motivations—that influence their gameplay decisions and emotional journey.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#impersonates -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#impersonates">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#Player"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#Protagonist"/>
        <rdfs:comment>Links a player to the protagonist they control, capturing the identification and narrative embodiment within the game.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#induces -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#induces">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/creativity/creon.owl#induces"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#Outcome"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#ReflectionState"/>
        <rdfs:comment>Links an outcome to the reflective process it triggers in the player.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#influences -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#influences">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#EmotionalResponse"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#Choice"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#ReflectionState"/>
        <rdfs:comment>Models the effect of an emotional response on the player’s decisions or reflection states.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#involves -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#involves">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#Event"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#Character"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#NPCs"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#Protagonist"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#isChoiceOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#isChoiceOf">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#Choice"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#Player"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#isContainedIn -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#isContainedIn">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#Event"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#Episode"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#isDrivenBy -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#isDrivenBy">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#EmotionalResponse"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#PlayerEmotion"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#PlayerMotivation"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#PlayerValue"/>
        <rdfs:comment>Links an emotional response to the player’s internal state—emotions, motivations, and values—that cause it.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#isExperiencedBy -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#isExperiencedBy">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#EmotionalResponse"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#Player"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#isInfluencedBy -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#isInfluencedBy">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#reaffirmsChoice"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#Choice"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#ReflectionState"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#EmotionalResponse"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#isInvolvedIn -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#isInvolvedIn">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/creativity/creon.owl#participantIn"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#Player"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#Event"/>
        <rdfs:comment></rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#isPartOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#isPartOf">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#Place"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#Location"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#leadsTo -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#leadsTo">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#Choice"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#Outcome"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#marks -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#marks">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#AcceptChoice"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#FinalDecision"/>
        <rdfs:comment>Signifies that an accepted choice leads to the final, confirmed decision of the episode.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#reaffirmsChoice -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#reaffirmsChoice">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#AcceptChoice"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#Choice"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#rejectsChoice -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#rejectsChoice">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#ReconsiderChoice"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#Choice"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#resets -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#resets">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#RewindMechanic"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#Event"/>
        <rdfs:comment>Indicates that the rewind mechanic resets an event, offering the player a second chance.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#resolvesTo -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#resolvesTo">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#FinalDecision"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#EpisodeEnd"/>
        <rdfs:comment>Links the final decision to the conclusion of the episode.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#resultsIn -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#resultsIn">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#ReflectionState"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#AcceptChoice"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#ReconsiderChoice"/>
        <rdfs:comment>Represents the decision taken during a reflection—either to accept or reconsider a previous choice.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#takesPlaceIn -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#takesPlaceIn">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/creativity/creon.owl#atLocation"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#Event"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#Location"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#triggers -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#triggers">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#Event"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#Choice"/>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.ontologydesignpatterns.org/ont/creativity/creon.owl#ActionDynamics -->

    <owl:Class rdf:about="http://www.ontologydesignpatterns.org/ont/creativity/creon.owl#ActionDynamics"/>
    


    <!-- http://www.ontologydesignpatterns.org/ont/creativity/creon.owl#CreativeProduct -->

    <owl:Class rdf:about="http://www.ontologydesignpatterns.org/ont/creativity/creon.owl#CreativeProduct"/>
    


    <!-- http://www.ontologydesignpatterns.org/ont/creativity/creon.owl#EmotionDynamics -->

    <owl:Class rdf:about="http://www.ontologydesignpatterns.org/ont/creativity/creon.owl#EmotionDynamics"/>
    


    <!-- http://www.ontologydesignpatterns.org/ont/creativity/creon.owl#EnvironmentalCondition -->

    <owl:Class rdf:about="http://www.ontologydesignpatterns.org/ont/creativity/creon.owl#EnvironmentalCondition"/>
    


    <!-- http://www.ontologydesignpatterns.org/ont/creativity/creon.owl#ExternalRandomEvent -->

    <owl:Class rdf:about="http://www.ontologydesignpatterns.org/ont/creativity/creon.owl#ExternalRandomEvent"/>
    


    <!-- http://www.ontologydesignpatterns.org/ont/creativity/creon.owl#PersonalSituation -->

    <owl:Class rdf:about="http://www.ontologydesignpatterns.org/ont/creativity/creon.owl#PersonalSituation"/>
    


    <!-- http://www.ontologydesignpatterns.org/ont/creativity/creon.owl#Resource -->

    <owl:Class rdf:about="http://www.ontologydesignpatterns.org/ont/creativity/creon.owl#Resource"/>
    


    <!-- http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Person -->

    <owl:Class rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Person"/>
    


    <!-- http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#AcceptChoice -->

    <owl:Class rdf:about="http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#AcceptChoice">
        <rdfs:comment>The player&apos;s decision to confirm the choice they just made.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#Character -->

    <owl:Class rdf:about="http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#Character"/>
    


    <!-- http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#Choice -->

    <owl:Class rdf:about="http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#Choice">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/creativity/creon.owl#ActionDynamics"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#EmotionalResponse -->

    <owl:Class rdf:about="http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#EmotionalResponse">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/creativity/creon.owl#PersonalSituation"/>
        <rdfs:comment>Represents the player’s emotional reaction to events and outcomes.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#Episode -->

    <owl:Class rdf:about="http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#Episode"/>
    


    <!-- http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#EpisodeEnd -->

    <owl:Class rdf:about="http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#EpisodeEnd">
        <rdfs:comment>Represents the conclusive state of an episode’s narrative, shaped by prior decisions and actions.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#Event -->

    <owl:Class rdf:about="http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#Event">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/creativity/creon.owl#ExternalRandomEvent"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#FinalDecision -->

    <owl:Class rdf:about="http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#FinalDecision">
        <rdfs:comment>The final confirmed choice made by the player in an episode, usually with lasting consequences on the narrative.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#InternalizedMeaning -->

    <owl:Class rdf:about="http://www.semanticweb.org/nadia/ontologies/2025/6/untitled-ontology-10#InternalizedMeaning">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/creativity/creon.owl#CreativeProduct"/>
        <rdfs:comment>A non-material but meaningful product created by the player as a result of their journey.</rdfs:comment>
