    </owl:Class>
    


    <!-- https://raw.githubusercontent.com/lisontology/Lisontology-/refs/heads/main/aligned_lisontology.rdf#Character -->

    <owl:Class rdf:about="https://raw.githubusercontent.com/lisontology/Lisontology-/refs/heads/main/aligned_lisontology.rdf#Character">
        <rdfs:comment>Any person in the game world who can participate in Events and influence the narrative.</rdfs:comment>
        <rdfs:label>Character</rdfs:label>
    </owl:Class>
    


    <!-- https://raw.githubusercontent.com/lisontology/Lisontology-/refs/heads/main/aligned_lisontology.rdf#Choice -->

    <owl:Class rdf:about="https://raw.githubusercontent.com/lisontology/Lisontology-/refs/heads/main/aligned_lisontology.rdf#Choice">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/creativity/creon.owl#ActionDynamics"/>
        <rdfs:comment>A decision point where the player influences the story&amp;apos;s direction, leading to different Outcomes.</rdfs:comment>
    </owl:Class>
    


    <!-- https://raw.githubusercontent.com/lisontology/Lisontology-/refs/heads/main/aligned_lisontology.rdf#EmotionalResponse -->

    <owl:Class rdf:about="https://raw.githubusercontent.com/lisontology/Lisontology-/refs/heads/main/aligned_lisontology.rdf#EmotionalResponse">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/creativity/creon.owl#PersonalSituation"/>
        <rdfs:comment>Represents the player’s emotional reaction to events and outcomes.</rdfs:comment>
        <rdfs:label>Emotional Response</rdfs:label>
    </owl:Class>
    


    <!-- https://raw.githubusercontent.com/lisontology/Lisontology-/refs/heads/main/aligned_lisontology.rdf#Episode -->

    <owl:Class rdf:about="https://raw.githubusercontent.com/lisontology/Lisontology-/refs/heads/main/aligned_lisontology.rdf#Episode">
        <rdfs:comment>A major chapter in the game&amp;apos;s story that groups together related Events and Choices (e.g,“Chrysalis”).</rdfs:comment>
        <rdfs:label>Episode</rdfs:label>
    </owl:Class>
    


    <!-- https://raw.githubusercontent.com/lisontology/Lisontology-/refs/heads/main/aligned_lisontology.rdf#EpisodeEnd -->

    <owl:Class rdf:about="https://raw.githubusercontent.com/lisontology/Lisontology-/refs/heads/main/aligned_lisontology.rdf#EpisodeEnd">
        <rdfs:comment>Represents the conclusive state of an episode’s narrative, shaped by prior decisions and actions.</rdfs:comment>
        <rdfs:label>Episode End</rdfs:label>
    </owl:Class>
    


    <!-- https://raw.githubusercontent.com/lisontology/Lisontology-/refs/heads/main/aligned_lisontology.rdf#Event -->

    <owl:Class rdf:about="https://raw.githubusercontent.com/lisontology/Lisontology-/refs/heads/main/aligned_lisontology.rdf#Event">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/creativity/creon.owl#ExternalRandomEvent"/>
        <rdfs:comment>A narrative occurrence or situation in the game tied to a specific Episode and Location.</rdfs:comment>
        <rdfs:label>Event</rdfs:label>
    </owl:Class>
    


    <!-- https://raw.githubusercontent.com/lisontology/Lisontology-/refs/heads/main/aligned_lisontology.rdf#FinalDecision -->

    <owl:Class rdf:about="https://raw.githubusercontent.com/lisontology/Lisontology-/refs/heads/main/aligned_lisontology.rdf#FinalDecision">
        <rdfs:comment>The final confirmed choice made by the player in an episode, usually with lasting consequences on the narrative.</rdfs:comment>
        <rdfs:label>Final Decision</rdfs:label>
    </owl:Class>
    


    <!-- https://raw.githubusercontent.com/lisontology/Lisontology-/refs/heads/main/aligned_lisontology.rdf#InternalizedMeaning -->

    <owl:Class rdf:about="https://raw.githubusercontent.com/lisontology/Lisontology-/refs/heads/main/aligned_lisontology.rdf#InternalizedMeaning">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/creativity/creon.owl#CreativeProduct"/>
        <rdfs:comment>A non-material but meaningful product created by the player as a result of their journey.</rdfs:comment>
        <rdfs:label>Internalized Meaning</rdfs:label>
    </owl:Class>
    


    <!-- https://raw.githubusercontent.com/lisontology/Lisontology-/refs/heads/main/aligned_lisontology.rdf#Location -->

    <owl:Class rdf:about="https://raw.githubusercontent.com/lisontology/Lisontology-/refs/heads/main/aligned_lisontology.rdf#Location">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/creativity/creon.owl#EnvironmentalCondition"/>
        <rdfs:comment>General setting in the game world that contains Places and hosts Episodes or Events (e.g, Blackwell Academy).</rdfs:comment>
        <rdfs:label>Location</rdfs:label>
    </owl:Class>
    


    <!-- https://raw.githubusercontent.com/lisontology/Lisontology-/refs/heads/main/aligned_lisontology.rdf#MajorChoice -->

    <owl:Class rdf:about="https://raw.githubusercontent.com/lisontology/Lisontology-/refs/heads/main/aligned_lisontology.rdf#MajorChoice">
        <rdfs:subClassOf rdf:resource="https://raw.githubusercontent.com/lisontology/Lisontology-/refs/heads/main/aligned_lisontology.rdf#Choice"/>
        <rdfs:comment>A high-impact player decision that significantly affects story progression or character development.</rdfs:comment>
        <rdfs:label>Major Choice</rdfs:label>
    </owl:Class>
    


    <!-- https://raw.githubusercontent.com/lisontology/Lisontology-/refs/heads/main/aligned_lisontology.rdf#MinorChoice -->

    <owl:Class rdf:about="https://raw.githubusercontent.com/lisontology/Lisontology-/refs/heads/main/aligned_lisontology.rdf#MinorChoice">
        <rdfs:subClassOf rdf:resource="https://raw.githubusercontent.com/lisontology/Lisontology-/refs/heads/main/aligned_lisontology.rdf#Choice"/>
        <rdfs:comment>A lower-impact decision that may influence relationships or small environmental details.</rdfs:comment>
        <rdfs:label>Minor Choice</rdfs:label>
    </owl:Class>
    


    <!-- https://raw.githubusercontent.com/lisontology/Lisontology-/refs/heads/main/aligned_lisontology.rdf#NPC -->

    <owl:Class rdf:about="https://raw.githubusercontent.com/lisontology/Lisontology-/refs/heads/main/aligned_lisontology.rdf#NPC">
        <rdfs:subClassOf rdf:resource="https://raw.githubusercontent.com/lisontology/Lisontology-/refs/heads/main/aligned_lisontology.rdf#Character"/>
        <rdfs:comment>A non-playable Character that the player interacts with (e.g., Chloe, David), often triggering new Events.</rdfs:comment>
        <rdfs:label>NPC</rdfs:label>
    </owl:Class>
    


    <!-- https://raw.githubusercontent.com/lisontology/Lisontology-/refs/heads/main/aligned_lisontology.rdf#Outcome -->

    <owl:Class rdf:about="https://raw.githubusercontent.com/lisontology/Lisontology-/refs/heads/main/aligned_lisontology.rdf#Outcome">
        <rdfs:comment>The resulting state or consequence of an Event or Choice, marking a new point in the narrative.</rdfs:comment>
        <rdfs:label>Outcome</rdfs:label>
    </owl:Class>
    


    <!-- https://raw.githubusercontent.com/lisontology/Lisontology-/refs/heads/main/aligned_lisontology.rdf#Place -->

    <owl:Class rdf:about="https://raw.githubusercontent.com/lisontology/Lisontology-/refs/heads/main/aligned_lisontology.rdf#Place">
        <rdfs:subClassOf rdf:resource="https://raw.githubusercontent.com/lisontology/Lisontology-/refs/heads/main/aligned_lisontology.rdf#Location"/>
        <rdfs:comment>A specific, detailed area within a larger Location where Events occur (e.g, Chloe&amp;apos&apos; Garage).</rdfs:comment>
        <rdfs:label>Place</rdfs:label>
    </owl:Class>
    


    <!-- https://raw.githubusercontent.com/lisontology/Lisontology-/refs/heads/main/aligned_lisontology.rdf#Player -->

    <owl:Class rdf:about="https://raw.githubusercontent.com/lisontology/Lisontology-/refs/heads/main/aligned_lisontology.rdf#Player">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Person"/>
        <rdfs:comment>Represents the real-world human agent who interacts with the game system. Represents the person responsile for the unfolding of the narrative.</rdfs:comment>
    </owl:Class>
    


    <!-- https://raw.githubusercontent.com/lisontology/Lisontology-/refs/heads/main/aligned_lisontology.rdf#PlayerEmotion -->

    <owl:Class rdf:about="https://raw.githubusercontent.com/lisontology/Lisontology-/refs/heads/main/aligned_lisontology.rdf#PlayerEmotion">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/creativity/creon.owl#EmotionDynamics"/>
        <rdfs:comment>Captures the immediate and fluctuating emotional reactions of the player.</rdfs:comment>
        <rdfs:label>Player Emotion</rdfs:label>
    </owl:Class>
    


    <!-- https://raw.githubusercontent.com/lisontology/Lisontology-/refs/heads/main/aligned_lisontology.rdf#PlayerMotivation -->

    <owl:Class rdf:about="https://raw.githubusercontent.com/lisontology/Lisontology-/refs/heads/main/aligned_lisontology.rdf#PlayerMotivation">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/creativity/creon.owl#PersonalAbility"/>
        <rdfs:comment>Describes the player’s inner drive and reasons for engaging with the game and making a choice over another.</rdfs:comment>
        <rdfs:label>Player Motivation</rdfs:label>
    </owl:Class>
    


    <!-- https://raw.githubusercontent.com/lisontology/Lisontology-/refs/heads/main/aligned_lisontology.rdf#PlayerValue -->

    <owl:Class rdf:about="https://raw.githubusercontent.com/lisontology/Lisontology-/refs/heads/main/aligned_lisontology.rdf#PlayerValue">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/creativity/creon.owl#PersonalAbility"/>
        <rdfs:comment>Represents the ethical and moral principles that guide the player’s choices.</rdfs:comment>
        <rdfs:label xml:lang="en">Player Value</rdfs:label>
    </owl:Class>
    


    <!-- https://raw.githubusercontent.com/lisontology/Lisontology-/refs/heads/main/aligned_lisontology.rdf#Protagonist -->

    <owl:Class rdf:about="https://raw.githubusercontent.com/lisontology/Lisontology-/refs/heads/main/aligned_lisontology.rdf#Protagonist">
        <rdfs:subClassOf rdf:resource="https://raw.githubusercontent.com/lisontology/Lisontology-/refs/heads/main/aligned_lisontology.rdf#Character"/>
        <rdfs:comment>The main player-controlled Character (Max Caulfield), whose decisions shape the unfolding story.</rdfs:comment>
        <rdfs:label>Protagonist</rdfs:label>
    </owl:Class>
    


    <!-- https://raw.githubusercontent.com/lisontology/Lisontology-/refs/heads/main/aligned_lisontology.rdf#ReconsiderChoice -->

    <owl:Class rdf:about="https://raw.githubusercontent.com/lisontology/Lisontology-/refs/heads/main/aligned_lisontology.rdf#ReconsiderChoice">
        <rdfs:comment>A situation in which the player chooses to undo a previous decision, often through the use of a game mechanic.</rdfs:comment>
        <rdfs:label>Reconsider Choice</rdfs:label>
    </owl:Class>
    


    <!-- https://raw.githubusercontent.com/lisontology/Lisontology-/refs/heads/main/aligned_lisontology.rdf#ReflectionState -->

    <owl:Class rdf:about="https://raw.githubusercontent.com/lisontology/Lisontology-/refs/heads/main/aligned_lisontology.rdf#ReflectionState">
        <rdfs:comment>A moment of internal reflection triggered by the consequences of a choice. Here, the player may evaluate or reconsider their actions.</rdfs:comment>
        <rdfs:label>Reflection State</rdfs:label>
    </owl:Class>
    


    <!-- https://raw.githubusercontent.com/lisontology/Lisontology-/refs/heads/main/aligned_lisontology.rdf#RewindMechanic -->

    <owl:Class rdf:about="https://raw.githubusercontent.com/lisontology/Lisontology-/refs/heads/main/aligned_lisontology.rdf#RewindMechanic">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/creativity/creon.owl#Resource"/>
        <rdfs:comment>A resource that allows the player to revisit and possibly alter past decisions.</rdfs:comment>
        <rdfs:label>Rewind Mechanic</rdfs:label>
    </owl:Class>
    


