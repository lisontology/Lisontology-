# EPISODE 1 RELATIONS

# Player
lis:Player1 rdf:type lis:Player ;
    lis:impersonates lis:MaxCaulfield .

#EVENT 1: Nathan's Gun
lis:nathansGun rdf:type lis:Event ;
    lis:takesPlaceIn lis:PrescottDormitory ;
    lis:isContainedIn lis:Episode1 ;
    lis:involves lis:MaxCaulfield, lis:NathanPrescott ;
    lis:triggers lis:reportNathan ;
    lis:elicits lis:emotionalResponseInitialFear .

lis:emotionalResponseInitialFear rdf:type lis:EmotionalResponse ;
    lis:isDrivenBy lis:playerEmotionFear, lis:playerValueSelfPreservation,
                lis:playerMotivationAvoidDanger ;
    lis:influences lis:reportNathan .

lis:reportNathan rdf:type lis:MajorChoice ;
    lis:isChoiceOf lis:Player1 ;
    lis:leadsTo lis:nathanGetsAngry .

lis:nathanGetsAngry rdf:type lis:Outcome ;
    lis:elicits lis:emotionalResponseThreatened ;
    lis:induces lis:ReflectionState1 .

lis:emotionalResponseThreatened rdf:type lis:EmotionalResponse ;
    lis:isDrivenBy lis:playerEmotionAnxiety,
                lis:playerValueResponsibility,
                lis:playerMotivationProtectOthers .

lis:ReflectionState1 rdf:type lis:ReflectionState ;
    lis:isInfluencedBy lis:emotionalResponseThreatened ;
    lis:resultsIn lis:AcceptChoice1 .

lis:AcceptChoice1 rdf:type lis:AcceptChoice ;
    lis:marks lis:FinalDecision1 .

# EVENT 2: Kate Marsh and David Madsen Confrontation
lis:kateMarshDavidMadsenConfrontation rdf:type lis:Event ;
    lis:takesPlaceIn lis:MainCampus ;
    lis:isContainedIn lis:Episode1 ;
    lis:involves lis:MaxCaulfield, lis:KateMarsh, lis:DavidMadsen ;
    lis:triggers lis:interveneKateDavidConfrontation, lis:takePhotoKateDavidConfrontation ;
    lis:elicits lis:emotionalResponseMoralConflict .

lis:emotionalResponseMoralConflict rdf:type lis:EmotionalResponse ;
    lis:isDrivenBy lis:playerEmotionAnxiety,
                lis:playerValueJustice,
                lis:playerMotivationProtectTheVulnerable ;
    lis:influences lis:interveneKateDavidConfrontation, lis:takePhotoKateDavidConfrontation .

lis:takePhotoKateDavidConfrontation rdf:type lis:MajorChoice ;
    lis:isChoiceOf lis:Player1 ;
    lis:leadsTo lis:kateFeelsExposed .

lis:kateFeelsExposed rdf:type lis:Outcome ;
    lis:elicits lis:emotionalResponseKateGuilt ;
    lis:induces lis:ReflectionState2 .

lis:emotionalResponseKateGuilt rdf:type lis:EmotionalResponse ;
    lis:isDrivenBy lis:playerEmotionGuilt,
                lis:playerValueEmpathy,
                lis:playerMotivationRepairRelationship .

lis:ReflectionState2 rdf:type lis:ReflectionState ;
    lis:isInfluencedBy lis:emotionalResponseKateGuilt ;
    lis:resultsIn lis:ReconsiderChoice1 .

lis:ReconsiderChoice1 rdf:type lis:ReconsiderChoice ;
    lis:rejectsChoice lis:takePhotoKateDavidConfrontation ;
    lis:enables lis:RewindMechanic1 .

lis:RewindMechanic1 rdf:type lis:RewindMechanic ;
    lis:resets lis:kateMarshDavidMadsenConfrontation .

lis:interveneKateDavidConfrontation rdf:type lis:MajorChoice ;
    lis:isChoiceOf lis:Player1 ;
    lis:leadsTo lis:kateTrustsMax .

lis:kateTrustsMax rdf:type lis:Outcome ;
    lis:elicits lis:emotionalResponseKateRelief ;
    lis:induces lis:ReflectionState3 .

lis:emotionalResponseKateRelief rdf:type lis:EmotionalResponse ;
    lis:isDrivenBy lis:playerEmotionEmpathy,
                lis:playerValueLoyalty,
                lis:playerMotivationProtectKate .

lis:ReflectionState3 rdf:type lis:ReflectionState ;
    lis:isInfluencedBy lis:emotionalResponseKateRelief ;
    lis:resultsIn lis:AcceptChoice2 .

lis:AcceptChoice2 rdf:type lis:AcceptChoice ;
    lis:marks lis:FinalDecision2 .

lis:FinalDecision2 rdf:type lis:FinalDecision .

